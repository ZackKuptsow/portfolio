{
  "name": "resume",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "check": "yarn lint && yarn format && yarn typecheck",
    "check:fix": "yarn lint:fix && yarn format:fix && yarn typecheck",
    "clean": "rm -rf .next && rm -rf dist && rm -rf coverage",
    "dev": "next dev --turbopack",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "install:clean": "rm -rf node_modules yarn.lock && yarn install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "next": "15.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.0.6",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.21",
    "eslint": "8.57.1",
    "eslint-config-next": "15.3.4",
    "eslint-import-resolver-typescript": "3.10.1",
    "eslint-plugin-import": "2.32.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@4.9.2",
  "workspaces": [
    "projects/*"
  ]
}
